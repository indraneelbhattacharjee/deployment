{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.jsx",
      "use": "@vercel/react"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/public/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "API_KEY": "@api_key"
  }
}

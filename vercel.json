{
    "version": 2,
    "builds": [
      { "src": "backend/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "/backend/server.js"
      },
      {
        "src": "/post_register",
        "dest": "/backend/server.js"
      },
      {
        "src": "/post_login",
        "dest": "/backend/server.js"
      },
      {
        "src": "/logout",
        "dest": "/backend/server.js"
      },
      {
        "src": "/",
        "dest": "/backend/server.js"
      }
    ]
  }
  
{
  "version": 2,
  "name": "your-project-name",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.jsx",
      "use": "@vercel/react"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/index.html"
    }
  ],
  "env": {
    "API_KEY": "@api_key"
  }
}
